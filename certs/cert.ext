DNS.1 = localhost
DNS.2 = *.*.svc.cluster.local
DNS.3 = *.kube-authx.svc.cluster.local
DNS.4 = *.kube-system.svc.cluster.local
DNS.5 = *.kube-neverdown.svc.cluster.local
DNS.6 = *.kube-discovery.svc.cluster.local
